{
    "name": "open20/open20-cms-template",
    "description": "Open20 CMS Template",
    "keywords": [
        "open20",
        "yii2",
        "framework",
        "advanced",
        "project template"
    ],
    "homepage": "http://www.open20.it/",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1.0",
        "open20/cmsbridge": "1.5.0",
        "open20/open2-cms-module-userauth": "1.6.0",
        "open20/amos-admin": "^2.18.2",
        "open20/amos-attachments": "^1.15.1",
        "open20/amos-audit": "~1.0.1",
        "open20/amos-comments": "1.11.0",
        "open20/amos-comuni": "1.3.3",
        "open20/amos-core": "^1.34.1",
        "open20/amos-cwh": "2.10.1",
        "open20/amos-dashboard": "1.10.3",
        "open20/design": "1.11.0",
        "open20/amos-email-manager": "1.3.0",
        "open20/amos-layout": "1.12.0",
        "open20/amos-my-activities": "1.5.3",
        "open20/amos-notify": "1.15.1",
        "open20/amos-privileges": "1.3.1",
        "open20/amos-seo": "1.1.1",
        "open20/amos-social-auth": "1.12.0",
        "open20/amos-tag": "1.10.0",
        "open20/amos-translation": "1.5.0",
        "open20/amos-upload": "1.0.9",
        "open20/amos-utility": "1.14.0",
        "open20/amos-wizflow": "1.0.7",
        "open20/amos-workflow": "1.4.0",
        "kartik-v/yii2-datecontrol": "@dev",
        "kartik-v/yii2-password": "@dev",
        "kartik-v/yii2-social": "@dev",
        "kartik-v/yii2-widgets": "@dev",
        "kartik-v/yii2-widget-select2": "2.2.4",
        "lajax/yii2-translate-manager": "1.*",
        "mdmsoft/yii2-admin": "~2.0",
        "schmunk42/yii2-giiant": "@stable",
        "yiisoft/yii2": "^2.0.28",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-bootstrap4-dropdown": "*",
        "bower-asset/jquery": "2.2.*@stable",
        "bower-asset/jquery.simplepagination": "dev-master",
        "luyadev/luya-core": "^1.0",
        "luyadev/luya-module-admin": "^3.6",
        "luyadev/luya-module-cms": "^3.3",
        "luyadev/luya-bootstrap3": "~1.0.0",
        "luyadev/luya-bootstrap4": "^1.0",
        "luyadev/luya-generic": "~1.0.0",
        "luyadev/luya-privacy": "^1.0",
        "samdark/yii2-psr-log-target": "^1.1",
        "samdark/sitemap": "~2.0",
        "trk/luya-uikit": "dev-master",
        "simialbi/yii2-schema-org": "^2.0.0",
        "open20/amp": "^1.1.7",
        "monolog/monolog": "~1.27.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*", 
        "symfony/browser-kit": "*"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false,
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "luyadev/luya-composer": true
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
